{
  "name": "gatsby-starter-minimal-ts",
  "version": "1.0.0",
  "private": true,
  "description": "A Gatsby starter written in TypeScript with no plugins and a single welcome page.",
  "author": "Jude Agboola",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "typegen:scss": "typed-scss-modules src --ignore 'src/scss/**'"
  },
  "license": "0BSD",
  "dependencies": {
    "gatsby": "^4.16.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-ionicons": "^4.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "gatsby-plugin-sass": "^5.16.0",
    "sass": "^1.52.3",
    "typed-scss-modules": "^7.0.1",
    "typescript": "^4.7.3"
  }
}
